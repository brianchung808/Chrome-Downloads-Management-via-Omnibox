{
  "manifest_version": 2,

  "name": "Search Downloads",
  "version": "1.0",
  "omnibox": { "keyword" : "ds" },
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "permissions": [
      "downloads",
